r\n/* Navbar */\r\nheader {\r\n  height: 80px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.logo {\r\n  font-family: \"Crimson Pro\";\r\n  font-size: 1.2rem;\r\n  color: white;\r\n  border: 1px solid white;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 4px 8px;\r\n  cursor: pointer;\r\n}\r\n\r\n.menu {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.menu li {\r\n  margin: 0 40px;\r\n}\r\n.menu a {\r\n  color: white;\r\n  transition: text-decoration 150ms ease;\r\n}\r\n.menu a:hover {\r\n  text-decoration: underline;\r\n}\r\n.menu .active {\r\n  font-weight: bold;\r\n}\r\n\r\n/* Home */\r\n.home {\r\n  margin-top: 140px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  height: calc(100% - 120px);\r\n}\r\n\r\n.home__text h1 {\r\n  font-family: \"Lora\", sans-serif;\r\n  font-size: var(--font-xxl);\r\n  justify-self: self-start;\r\n}\r\n.home__text .description p {\r\n  font-size: var(--font-l);\r\n  margin: 40px 0;\r\n}\r\n\r\n.btn {\r\n  padding: 4px;\r\n  background: transparent;\r\n  outline: none;\r\n  border: none;\r\n  color: white;\r\n  font-size: 1.2rem;\r\n  cursor: pointer;\r\n  transition: opacity 150ms ease;\r\n}\r\n.btn-icon {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.btn-icon svg {\r\n  margin-left: 20px;\r\n}\r\n.btn:hover {\r\n  opacity: 0.8;\r\n}\r\n.home__image {\r\n  position: relative;\r\n}\r\n.realisation {\r\n  max-width: 100%;\r\n}\r\n.artist {\r\n  position: absolute;\r\n  left: 30%;\r\n  right: 5%;\r\n  bottom: -28%;\r\n  background-color: var(--clr-bg);\r\n  padding: 1.2rem 1.5rem;\r\n}\r\n.artist__infos {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.artist__text {\r\n  margin-left: 1.2rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  margin-bottom: 1.2rem;\r\n}\r\n.name {\r\n  font-size: 1.1rem;\r\n  font-weight: bold;\r\n}\r\n.job {\r\n  font-size: 0.9rem;\r\n}\r\n.avatar {\r\n  width: 50px;\r\n  height: 50px;\r\n  border-radius: 50%;\r\n}\r\n\r\n.realisation_desc {\r\n  font-family: \"Lora\", sans-serif;\r\n  font-size: 1.3rem;\r\n  font-weight: bold;\r\n}\r\n\r\nfooter {\r\n  height: 40px;\r\n}\r\n\r\n/* MEDIA QUERIES */\r\n\r\n@media screen and (max-width: 799px) {\r\n  .container {\r\n    height: 100%;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"8fb595c91c29c8859e43e87d6ede8de6","cacheData":{"env":{}}}